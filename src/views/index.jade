        div.navbar.navbar-inverse.navbar-fixed-top
            div.navbar-inner
                div.container
                    a.btn.btn-navbar(data-toggle="collapse",data-target=".nav-collapse")
                        span.icon-bar
                        span.icon-bar
                        span.icon-bar
                    a.brand(href="#/") #{title}
                    div.nav-collapse.collapse
                        ul.nav(ng-controller="NavigationCtrl")
                            li(ng-class="isCurrent('/')"): a(href="#/") Home
                            li(ng-class="isCurrent('/about')"): a(href="#/about") About
                            li(ng-class="isCurrent('/contact')"): a(href="#/contact") Contact
                            li.dropdown
                                a.dropdown-toggle(href="#",data-toggle="dropdown") Dropdown <b class="caret"></b>
                                ul.dropdown-menu
                                    li: a(href="#/?action=action") Action
                                    li: a(href="#/?action=Another%20Action") Another Action
                                    li: a(href="#/?action=Something%20else") Something else
                                    li.divider
                                    li.nav-header Nav header
                                    li: a(href="#/?action=Separated%20Link") Separated Link
                                    li: a(href="#/?action=One%20more%20separated%20link") One more separated link
                        form.navbar-form.pull-right
                            input.span2(type="text",placeholder="Email")
                            input.span2(type="password",placeholder="Password")
